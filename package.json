    "express": "^4.16.4",
    "express-favicon": "^2.0.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.17.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "flow-bin": "^0.65.0",
