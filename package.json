    "react-router-dom": "^4.2.2",
    "react-scripts": "0.9.x",
    "recharts": "^1.7.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "lodash": "^4.17.11",
    "formik": "^1.0.3",
    "yup": "^0.26.2",
